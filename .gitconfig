[user]
	name = Yasuhiro WATANABE
	email = info@nabeyasu.com
[init]
	defaultBranch = main
[core]
	excludesfile = /Users/yasuhiro/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[commit]
	template = /Users/yasuhiro/.stCommitMsg
[merge]
	ff = false
